icons = {
    "thunderstorms": base64.decode(""),
    "breezy": base64.decode(""),
    "clear": base64.decode(""),
    "clear-night": base64.decode(""),
    "mostlyclear": base64.decode(""),
    "mostlyclear-night": base64.decode(""),
    "partlycloudy": base64.decode(""),
    "partlycloudy-night": base64.decode(""),
    "mostlycloudy": base64.decode(""),
